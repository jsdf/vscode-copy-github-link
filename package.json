{
  "publisher": "XiaochenCui",
  "name": "copy-github-link",
  "displayName": "Copy GitHub Link",
  "description": "Copy the GitHub link of a code snippet, no extra configuration needed.",
  "repository": {
    "type": "git",
    "url": "https://github.com/XiaochenCui/vscode-copy-github-link"
  },
  "version": "0.9.11",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copy-github-link.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "copy-github-link.copyGitHubLink",
        "title": "Copy GitHub Link"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "copy-github-link.copyGitHubLink",
          "group": "9_cutcopypaste"
        }
      ]
    }
  },
  "icon": "./resources/images.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "simple-git": "^3.25.0"
  }
}
